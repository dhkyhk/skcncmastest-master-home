<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.primary.employeemapper">
    <select id="selectone" parameterType="skcnc.msa3.framework.database.MetaHashMap" resultType="skcnc.msa3.framework.database.MetaHashMap">
        <![CDATA[
        select id
             , password
          from employee
         where id = #{id}
        ]]>
    </select>
    <select id="selectarray" parameterType="skcnc.msa3.framework.database.MetaHashMap" resultType="skcnc.msa3.framework.database.MetaHashMap">
        <![CDATA[
        select id
             , password
          from employee
        ]]>
    </select>
</mapper>